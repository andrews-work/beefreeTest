{
    "name": "Laravel+Vue",
    "build": {
      "context": ".",
      "dockerfile": "Dockerfile"
    },
    "runArgs": ["--network=host"],
    "portsAttributes": {
      "8000": {
        "label": "Laravel",
        "onAutoForward": "open-preview"
      },
      "5173": {
        "label": "Vite",
        "onAutoForward": "notify"
      },
      "3306": {
        "label": "MySQL",
        "onAutoForward": "notify"
      }
    },
    "forwardPorts": [8000, 5173, 3306],
    "postCreateCommand": "gitpod tasks run setup"
  }
